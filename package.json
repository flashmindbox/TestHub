{
  "name": "testhub",
  "version": "1.0.0",
  "description": "Centralized testing hub for multiple projects",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "test": "playwright test",
    "test:e2e": "playwright test tests/e2e",
    "test:api": "playwright test tests/api",
    "test:visual": "playwright test tests/visual",
    "test:a11y": "playwright test tests/accessibility",
    "test:perf": "playwright test tests/performance",
    "test:smoke": "playwright test --grep @smoke",
    "test:critical": "playwright test --grep @critical",
    "test:studytab": "playwright test --grep @studytab",
    "test:auth": "playwright test --grep @auth",
    "test:local": "cross-env ENV=local playwright test",
    "test:staging": "cross-env ENV=staging playwright test",
    "test:prod": "cross-env ENV=production playwright test --grep @readonly",
    "test:ci": "cross-env CI=true playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:trace": "playwright test --trace on",
    "test:headed": "playwright test --headed",
    "visual:update": "playwright test tests/visual --update-snapshots",
    "report": "playwright show-report reports/html",
    "report:open": "open reports/html/index.html || start reports/html/index.html",
    "setup": "playwright install && tsx scripts/setup.ts",
    "add-project": "tsx scripts/add-project.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf reports .auth dist"
  },
  "keywords": [
    "testing",
    "playwright",
    "e2e",
    "automation"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@faker-js/faker": "^10.2.0",
    "@playwright/experimental-ct-react": "^1.57.0",
    "@playwright/test": "^1.57.0",
    "@tanstack/react-query": "^5.90.19",
    "@types/node": "^25.0.9",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "rimraf": "^5.0.10",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "zod": "^4.3.5"
  }
}
